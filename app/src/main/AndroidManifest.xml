<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lihh.animationapplication" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SocketTestActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_fatpo" >
        </activity>
        <activity
            android:name=".FatpoActivity"
            android:label="@string/title_activity_buffer_fly" >
        </activity>
        <activity
            android:name=".BufferFlyActivity"
            android:label="@string/title_activity_animator_test" >
        </activity>
        <activity
            android:name=".AnimatorTestActivity"
            android:label="@string/title_activity_surface" >
        </activity>
        <activity
            android:name=".SurfaceActivity"
            android:label="@string/title_activity_socket_test" >
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".MyIntentService"
            android:exported="false" >
        </service>
    </application>

</manifest>
